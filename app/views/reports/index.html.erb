
<div style = "position:relative; left:350px; top:250px; ">

<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Date</th>
      <th>Doctor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.label %></td>
        <td><%= report.date %></td>
        <td><%= report.doctor_id %></td>
        <td><%= link_to 'Show', report %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>